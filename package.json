{
  "name": "syfr-sdk",
  "version": "1.0.0",
  "main": ".src/index.js",
  "license": "none",
  "private": true,
  "scripts": {
    "build": "npm run-script generate",
    "generate": "graphql-codegen --require dotenv/config --config src/codegen.yml dotenv_config_path=.env"
  },
  "exports": {
    "./graphqlRequest": {
      "import": "./dist/graphqlRequest.ts",
      "require": "./dist/graphqlRequest.ts"
