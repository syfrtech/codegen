    "./reactQuery": {
      "import": "./dist/reactQuery.ts",
      "require": "./dist/reactQuery.ts"
    }
  },
  "dependencies": {
    "graphql-request": "^3.5.0",
    "graphql-tag": "^2.12.5",
    "jose": "^3.14.3",
    "react-query": "^3.19.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.8",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-codegen/typescript-graphql-request": "^3.2.5",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@graphql-codegen/typescript-react-query": "^1.3.5",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.1"
  }
}
